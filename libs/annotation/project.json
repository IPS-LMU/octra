{
  "name": "annotation",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/annotation/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/annotation",
        "main": "libs/annotation/src/index.ts",
        "tsConfig": "libs/annotation/tsconfig.lib.json",
        "project": "libs/annotation/package.json",
        "entryFile": "libs/annotation/src/index.ts",
        "compiler": "babel",
        "external": ["typescript"],
        "format": ["cjs", "esm"]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs annotation {args.ver} {args.tag}"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/annotation/**/*.ts"]
      }
    }
  },
  "tags": []
}
