{
  "name": "octra",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "octra",
  "sourceRoot": "apps/octra/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/octra",
        "index": "apps/octra/src/index.html",
        "main": "apps/octra/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/octra/tsconfig.app.json",
        "allowedCommonJsDependencies": [
          "jodit-angular",
          "ajv",
          "x2js",
          "konva",
          "platform"
        ],
        "assets": [
          "apps/octra/src/assets",
          "apps/octra/src/config",
          "apps/octra/src/media",
          "apps/octra/src/LICENSE.txt",
          "apps/octra/src/.htaccess"
        ],
        "styles": [
          "node_modules/video.js/dist/video-js.min.css",
          "apps/octra/src/styles.scss"
        ],
        "scripts": [
          "apps/octra/src/assets/js/modernizr.js",
          "node_modules/hammerjs/hammer.min.js"
        ],
        "extractLicenses": true
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "6mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "6kb",
              "maximumError": "10kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/octra/src/environments/environment.ts",
              "with": "apps/octra/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "fileReplacements": [
            {
              "replace": "apps/octra/src/environments/environment.ts",
              "with": "apps/octra/src/environments/environment.dev.ts"
            }
          ]
        },
        "public-development": {
          "outputHashing": "none",
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "fileReplacements": [
            {
              "replace": "apps/octra/src/environments/environment.ts",
              "with": "apps/octra/src/environments/environment.public.dev.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "octra:build:production",
          "port": 5321
        },
        "development": {
          "browserTarget": "octra:build:development",
          "port": 5321
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "octra:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/octra/**/*.ts", "apps/octra/**/*.html"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/octra/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "octra:build"
      }
    }
  }
}
