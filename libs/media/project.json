{
  "name": "media",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/media/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/media",
        "main": "libs/media/src/index.ts",
        "tsConfig": "libs/media/tsconfig.lib.json",
        "assets": [],
        "project": "libs/media/package.json",
        "compiler": "tsc"
      }
    },
    "package": {
      "executor": "@nrwl/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "project": "libs/media/package.json",
        "outputPath": "dist/libs/media",
        "entryFile": "libs/media/src/index.ts",
        "tsConfig": "libs/media/tsconfig.lib.json",
        "compiler": "babel",
        "external": ["typescript"],
        "format": ["cjs", "esm"]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs media {args.ver} {args.tag}"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/media/**/*.ts"]
      }
    }
  },
  "tags": []
}
