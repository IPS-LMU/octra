    "build-prod": "node ./build.js dev=false isUpdate=true url=/apps/octra/octra/",
    "build-extern": " nx build annotation --with-deps && rm -rf extern/* && cp -rf ./dist/libs ./extern/libs",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
